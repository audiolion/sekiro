    "react-dom": "16.8.5",
    "react-hot-loader": "4.8.0",
    "responsive-gamepad": "1.2.0",
    "transducers.js": "^0.3.2"
  },
  "devDependencies": {
    "@babel/core": "7.4.0",
    "@babel/preset-react": "7.0.0",
    "@types/classnames": "^2.2.7",
    "@types/html-webpack-plugin": "3.2.0",
    "@types/jest": "24.0.11",
    "@types/react": "16.8.5",
    "@types/react-dom": "16.8.3",
